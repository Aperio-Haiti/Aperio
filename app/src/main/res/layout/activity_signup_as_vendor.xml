<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Signup_as_vendor"
    android:background="#403F4C">
    <androidx.appcompat.widget.Toolbar
        android:id="@+id/tb_fragmen1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="?attr/colorPrimary"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        >

        <ImageButton
            android:id="@+id/button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="#00A85151"
            android:gravity="center"
            android:onClick="backtoprevious"
            android:text="Button"
            tools:src="@drawable/ic_arrow_back_black_24dp" /></androidx.appcompat.widget.Toolbar>
    <FrameLayout
        android:id="@+id/slideViewPager"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />
</RelativeLayout>